@prefix cpvv: <http://purl.org/vocab/cpsv#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://vocab.belgif.be/shacl/as#> a owl:Ontology;
  rdfs:comment "Authentischen Quellen SHACL rules"@de, "Authentic Sources SHACL rules"@en, 
		"Sources Authentiques SHACL rules"@fr, "Authentieke bronnen SHACL rules"@nl;
  rdfs:label "AS model"@de, "AS model"@en, "AS model"@fr, "AS model"@nl;
  owl:versionInfo "Draft 2019-02-20 (incomplete)".

<http://vocab.belgif.be/shacl/as#ContactShape> a sh:NodeShape;
  sh:property [
      sh:datatype xsd:string;
      sh:minCount 0;
      sh:path schema:name
    ], [
      sh:datatype xsd:string;
      sh:maxCount 1;
	  sh:minCount 1;
      sh:path schema:email
    ], [
      sh:datatype xsd:string;
	  sh:maxCount 1;
	  sh:minCount 0;
      sh:path schema:telephone
    ];
  sh:targetClass schema:ContactPoint.

<http://vocab.belgif.be/shacl/as#FormalFrameworkShape> a sh:NodeShape;
  sh:property [
      sh:class foaf:Document;
      sh:minCount 1;
      sh:nodeKind sh:IRI;
      sh:path foaf:page
    ];
  sh:targetClass cpsv:FormalFramework .
